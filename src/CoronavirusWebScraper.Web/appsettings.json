{
  "MongoDbSettings": {
    "ConnectionString": "",
    "DatabaseName": "CovidStatisticsDb"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "CovidWebScraper Health Check",
        "Uri": "http://localhost:4569/health"
      },
      {
        "Name": "CovidWebScraper Health Check port 5000",
        "Uri": "http://localhost:5000/health"
      }
    ],
    "EvaluationTimeOnSeconds": 30,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "HostedService": {
    "Hours": 24
  },
  "AllowedHosts": "*"
}
